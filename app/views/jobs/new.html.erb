<h1>Joblist Submission</h1>
<% if !@job.errors.empty? %>
  <div class="alert alert-error">
    <ul>
      <% @job.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

<div class="well">
  <p>Please upload your joblist</p>
  <%= form_for @job, html: { multipart: true } do |f| %>
    <%= f.label :Email %>
    <%= f.text_field :name %>
    <!--<%= f.label :joblist %>-->
    <%= f.file_field :joblist %>
    <br />
    <%= f.submit "Upload", class: "btn btn-primary" %>
  <% end %>
</div>
    
